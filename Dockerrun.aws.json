{
    "AWSEBDockerrunVersion":3 ,
    "containerDefinitions": [
        {
            "name": "nginx",
            "image": "nolancustodio/personal-site-nginx:1",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": "client",
            "memory": 100
        },
        {
            "name": "client",
            "image": "nolancustodio/personal-site-client:1",
            "hostname": "client",
            "essential": false,
            "memory": 300
        }
    ]
}